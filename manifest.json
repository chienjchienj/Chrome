{
   "background": {
      "scripts": [ 
        "js/libs/jquery/jquery_1.9.1.min.js",
         "js/libs/async/async.js",
         "js/libs/mixpanel/mixpanel.js",
         "js/background/schemas/sharedkrake_template.js",
         "js/background/schemas/params.js",
         "js/background/helpers/sharedkrake_helper.js",
         "js/background/helpers/cookie_helper.js",
         "js/background/helpers/session_manager.js",
         "js/background/helpers/mixpanel_event.js",
         "js/background/helpers/pattern_matcher.js",
         "js/background/models/columns.js",
         "js/background/background.js"
      ]
   },
   "browser_action": {
      "default_icon": "images/krake_icon_disabled_24.png",
      "default_title": "Get Data"
   },

   "content_scripts": [ {
      "matches"   : [ "<all_urls>" ],
      "css": [ "css/content.css" ], 
      "js": [ 
        "js/content/load_time.js",
        "js/libs/jquery/jquery_1.9.1.min.js",
        "js/libs/jquery/jquery_ui_1.10.3.js", 
        "js/content/behavioral_mode.js",
        "js/content/content.js" 
      ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_end"
   } ],
   "web_accessible_resources": [
    "images/bin.png",
    "images/save.png",
    "images/link.png",
    "images/edit.png",
    "images/close.png",
    "html/panel.html"
  ],
   "description": "Extract data with Krake",
   "icons": {
      "128": "images/krake_icon_128.png",
      "16": "images/krake_icon_16.png",
      "24": "images/krake_icon_24.png",
      "32": "images/krake_icon_32.png",
      "48": "images/krake_icon_48.png"
   },
   "manifest_version": 2,
   "name": "Krake.IO",
   "permissions": [ "contextMenus", 
                    "tabs", 
                    "http://*/*", 
                    "https://*/*",
                    "cookies"],
   "content_security_policy": "script-src 'self' https://cdn.mxpnl.com; object-src 'self'",
   "version": "1.3.2"
}
